{
    "compilerOptions": {
        "target": "es2017",
        "module": "commonjs",
        "moduleResolution": "node",
        "strictNullChecks": true,
        "esModuleInterop": true,
        "noImplicitAny": false,
        "jsx": "react-jsx",
        "baseUrl": "./src",
        "paths": {
        "@/*": ["*"],
        "components/*": ["components/*"],
        "pages/*": ["pages/*"],
        "containers/*": ["containers/*"],
        "data/*": ["data/*"],
        "helpers/*": ["helpers/*"],
        "hooks/*": ["hooks/*"]
        }
    },
    "types": [
        "react",
        "react-dom",
        "antd"
      ],
    "include": [
        "src"
    ]
}